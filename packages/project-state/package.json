{
	"name": "@app/project-state",
	"version": "0.0.25",
	"private": true,
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"scripts": {
		"bench": "vitest bench"
	},
	"exports": {
		"./package.json": "./package.json",
