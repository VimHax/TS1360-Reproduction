			"types": "./dist/lib/operation.d.ts",
			"import": "./dist/lib/operation.js",
			"default": "./dist/lib/operation.js"
		}
	},
	"dependencies": {
		"prosemirror-state": "1.4.3",
		"tslib": "2.8.1"
